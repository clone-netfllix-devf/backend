    "graphql": "^0.13.2",
    "jsonwebtoken": "^8.2.2",
    "mongoose": "^5.1.3",
    "nodemon": "^1.17.5",
    "openpay": "^1.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1"
  }
}
